{
  "name": "create-libton",
  "version": "0.12.1",
  "description": "Create modern lib with no build configuration.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/libton-project/libton",
    "directory": "packages/create-libton"
  },
  "license": "MIT",
  "files": [
    "src",
    "dist",
    "generator"
  ],
  "engines": {
    "node": ">=10"
  },
  "bin": {
    "create-libton": "./dist/bin/create-libton.js"
  },
  "scripts": {
    "clean": "rimraf dist && rimraf generator/sao",
    "prebuild": "yarn clean",
    "build": "tsc && mv dist/sao generator/",
    "prepublishOnly": "yarn build",
    "test": "jest --passWithNoTests",
    "pre-commit": "lint-staged",
    "format": "prettier --write 'src/**/*.{js,ts,md}'",
    "format:check": "prettier --list-different 'src/**/*.{js,ts,md}'"
  },
  "lint-staged": {
    "src/**/*.{js,ts,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.1",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@libton/share": "^0.12.1",
    "commander": "^4.0.1",
    "find-up": "^4.1.0",
    "latest-version": "^5.1.0",
    "sao": "^1.7.0"
  },
  "gitHead": "20cc3c553e50b744d78b33a9d683a9113664d76c"
}
